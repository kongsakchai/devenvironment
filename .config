#!/bin/bash

# Scripts
source $WORKSPACE/workspaces/.scripts/workspaces.sh

# Executable
export PATH=$PATH:$WORKSPACE/workspaces/.scripts/exec
export PATH=$PATH:$WORKSPACE/workspaces/.scripts/exec/podman # Use docker command with podman

# Aliases
alias note="vim ~/note.md"                         # Open note file
alias wsconfig="vim $WORKSPACE/workspaces/.config" # Open workspace config
